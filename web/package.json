    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "elysia": "^1.0.12",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^1.0.5",
    "phoenix": "^1.7.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/bun": "^1.0.8",
    "typescript": "^5.3.3"
  }
}
